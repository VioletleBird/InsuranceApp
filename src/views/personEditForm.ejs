<h2>Upravit pojištěnce</h2>
            <form class="add-new-name action="">
                <div class="inputs">
                    <label for="firstName">Jméno</label>
                    <input type="text" id="firstName" name="firstName" value="<%= person.firstName %>">
                </div>
                <div class="inputs">
                    <label for="lastName">Příjmení</label>
                    <input type="text" id="lastName" name="lastName" value="<%= person.lastName %>">
                </div>
                <div class="inputs form-row">
                    <label for="birthDate">Datum narození</label>
                    <input type="date" id="birthDate" name="birthDate" value="<%= person.birthDate %>">
                </div>
                <div class="inputs">
                    <label for="email">E-mail</label>
                    <input type="text" id="email" name="email" value="<%= person.email %>">
                </div>
                <div class="inputs">
                    <label for="mobile">Telefon</label>
                    <input type="text" id="mobile" name="mobile" value="<%= person.mobile %>">
                </div>
                <div class="inputs">
                    <label for="street">Ulice</label>
                    <input type="text" id="street" name="street" value="<%= person.address[0] %>">
                </div>
                <div class="inputs">
                    <label for="streetNum">Č.p.</label>
                    <input type="text" id="streetNum" name="streetNum" value="<%= person.address[1] %>">
                </div>
                <div class="inputs">
                    <label for="zipcode">PSČ</label>
                    <input type="text" id="zipcode" name="zipcode" value="<%= person.address[2] %>">
                </div>
                <div class="inputs">
                    <label for="city">Město</label>
                    <input type="text" id="city" name="city" value="<%= person.address[3] %>">
                </div>
                <button class="btns btn-submit btn-person-edit" value="<%= person._id %>">Uložit</button>
            </form>